{
    "version": "2.0",
    "extensions": {
        "http": {
            "dynamicThrottlesEnabled": false
        }
    },
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        },
        "logLevel": {
            "default": "Information",
            "Microsoft": "Warning"
        }
    }
}
